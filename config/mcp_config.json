{
  "mcpServers": {
    "yahoo-finance-obot": {
      "type": "http",
      "url": "http://localhost:8080/mcp-connect/default-yahoo-finance-mcp",
      "headers": {
        "Content-Type": "application/json",
        "task_id": "YOUR_APP_ID_HERE"
      },
      "description": "Yahoo Finance MCP Server via Obot",
      "priority": 1,
      "enabled": true
    }
  },
  "fallbackStrategy": {
    "enabled": true,
    "maxRetries": 3,
    "retryDelay": 2000,
    "useDirectFetch": true,
    "description": "If MCP fails, fallback to direct yfinance library"
  },
  "caching": {
    "enabled": true,
    "duration": 24,
    "unit": "hours"
  }
}
