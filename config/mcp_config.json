{
  "mcpServers": {
    "yfinance-primary": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-yahoo-finance"
      ],
      "description": "Primary Yahoo Finance MCP Server - Official ModelContext Protocol",
      "priority": 1,
      "enabled": true
    },
    "yfinance-agentx": {
      "command": "npx",
      "args": [
        "-y",
        "@agentx-ai/mcp-server-yahoo-finance"
      ],
      "description": "AgentX-ai Yahoo Finance Server - Alternative implementation",
      "priority": 2,
      "enabled": true
    },
    "yfinance-alex": {
      "command": "npx",
      "args": [
        "-y",
        "@alex2yang97/mcp-yahoo-finance"
      ],
      "description": "Alex2Yang97 Yahoo Finance MCP - Community server",
      "priority": 3,
      "enabled": true
    },
    "yfinance-leon": {
      "command": "npx",
      "args": [
        "-y",
        "@leoncuhk/mcp-yahoo-finance"
      ],
      "description": "leoncuhk MCP Yahoo Finance - Extended features",
      "priority": 4,
      "enabled": true
    },
    "yfinance-zentickr": {
      "command": "npx",
      "args": [
        "-y",
        "@zentickr/yahoo-query-mcp"
      ],
      "description": "Zentickr Yahoo-query MCP - Query-focused implementation",
      "priority": 5,
      "enabled": true
    }
  },
  "fallbackStrategy": {
    "enabled": true,
    "maxRetries": 3,
    "retryDelay": 2000,
    "useDirectFetch": true,
    "description": "If all MCP servers fail, fallback to direct yfinance library"
  },
  "caching": {
    "enabled": true,
    "duration": 24,
    "unit": "hours"
  }
}
